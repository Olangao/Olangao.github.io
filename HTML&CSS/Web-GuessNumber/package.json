{
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4"
  },
  "prettier": {
    "tabWidth": 2,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 120,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "tabWidth": 4
        }
      }
    ]
  },
  "dependencies": {
    "flowbite": "^2.4.1"
  }
}
