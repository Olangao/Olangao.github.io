{
  "devDependencies": {
    "daisyui": "^4.12.10",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.6"
  },
  "prettier": {
    "tabWidth": 2,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 120,
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "singleQuote": true,
          "tabWidth": 4
        }
      }
    ]
  }
}
